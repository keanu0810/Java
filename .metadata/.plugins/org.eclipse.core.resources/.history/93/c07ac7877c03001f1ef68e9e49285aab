package com.kh.variable.pre;

public class EnumPre {
//필드
	// enum 무지개 {빨~보};
	enum 무지개 {빨, 주, 노, 초, 파, 남, 보};
	
	//enum으로 소비자 {학생,  직장인
	//enum으로 판매자 {소매업, 도매업, 중매업}
	
//생성자 : 필수
	public EnumPre() {
		
		
	}
//메서드
	//void
	public void 메서드1() {
		// 빨~보 출력		
		
	//void 메서드2 를 만들어서 소비자와 판매자를 for-each 사용해서 각각 출력
		
	//소비자 학생
	}
		
		//메인메서드
	public static void main(String[] args) {
		
	}
		
		
		
		
		
		
		
		//장바구니 = 배열에 담기
		무지개[] 배열 = 무지개.values(); // enum으로 작성한 값을 가지고 옴
		//for-each
		for(무지개 무 : 배열) {
			System.out.println(무);
		}
	}
	// 빨~보 출력
}
