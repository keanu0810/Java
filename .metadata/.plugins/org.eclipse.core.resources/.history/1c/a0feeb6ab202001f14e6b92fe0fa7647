package com.kh.FileEx;

import java.io.File;

public class FilePre {

	public FilePre() {

	}

	public void 메서드1() {
		File 텍스트파일 = new File("c:/Users/user1/Desktop/newFFF/파일생성.tet");
		
		if(텍스트파일.exists()) {
			System.out.println("파일이 존재합니다.");
		} else { //존재하지 않는다면
			//만약에 파일 만ㄴ들기 성공!
			텍스트파일.createNewFile();
		}
	}

	public void 메서드2() {

	}

	// main 메서드 하나 만들기
	public static void main(String[] args) {
		FilePre 파일 = new FilePre();
		파일.메서드1();
	}
}
