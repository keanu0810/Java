package sup1.controller;

public class Score {
//필드
	private int 국어;
	private int 영어;
	private int 수학;
	private int 총점;
	private int 평균;

//메서드
	
	
	//필수생성자
	
	//void 점수
	public void 점수() {
		총점 = 국어 + 영어 + 수학;
		평균 = 총점 / 3.0;
		
		//평균 50점 이상이면 합격
		//삼항연산자로 합격 불합격
		//각 점수가 50점 이상 평균 50점 이상
		//출력문으로 국어영어 수학점
		//상항연산자에 대한 결과 출력
	}
}
