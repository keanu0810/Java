package com.kh.ThreadEx;

public class Main {
	private void min() {
		
		
		ThreadEx 스레드 = new ThreadEx();
		스레드.start();
		
		//System.out.println("1차 스레드 끝 Ex2 스레드 시작!");
		
		//for 총 3개의 스레드를 생성해서 실행
		for(int i =0; i <3; i++) {
		//ThreadEx2의 부모이기 떄문에 Thread 왼쪽에서 작성하는 것이 가능	
			Thread t = new ThreadEx2(i);
			t.start();
		}
		System.out.println("메인 끝 ~~~!");
		}
	}
	
	
