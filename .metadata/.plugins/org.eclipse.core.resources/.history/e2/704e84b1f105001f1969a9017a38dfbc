package com.kh.StackEx;

import java.util.LinkedList;
import java.util.Queue;

public class QueuePre {
	public static void main(String[] args) {
		Queue<Integer> 큐 = new LinkedList<>();
		
		//offer 80 60 30 20
		큐.offer(80);
		큐.offer(60);
		큐.offer(40);
		큐.offer(20);
		//poll 데이터 확인 및 제거
		int 맨앞데이터 = 큐.poll();
		System.out.println(맨앞데이터);
		
		//peek 맨 앞 데이터 확인
		int 데이터확인 = 큐.peek();
		System.out.println(데이터확인);
		
		//isEmpty 비어있는지 확인
		boolean 빈값확인 = 큐.isEmpty();
		System.out.println(+ 빈값확인 );
	}
}
